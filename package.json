{
  "name": "gpda-api",
  "version": "1.0.0",
  "description": "Desenvolvendo API que ser√° implementada ao gpda-web",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "dev": "yarn service:up && ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "style:check": "prettier --check [files]",
    "style:fix": "prettier --write [files]",
    "service:up": "docker compose -f infra/compose.yaml up -d",
    "service:stop": "docker compose -f infra/compose.yaml stop",
    "service:down": "docker compose -f infra/compose.yaml down"
  },
  "repository": "https://github.com/Projeto-Stratum/gpda-api.git",
  "author": "Nicolas 'grecoww' Greco and Guilherme 'Alkoarism' Longo",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "nodemailer": "^6.9.9",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
